 }\r\n\r\n                    .header-title {\r\n                        padding-left: 2.9em;\r\n                        height: 2.5em;\r\n                        line-height: 3em;\r\n\r\n                        font-family: 'Trebuchet', sans-serif;\r\n                        font-size: 1.25vw;\r\n                        color: #b9b9b9;\r\n                    }\r\n                }\r\n\r\n                .item-main {\r\n                    position: relative;\r\n                }\r\n\r\n                .item-main h1 {\r\n                    padding: 0 0 0 1em;\r\n\r\n                    font-family: 'Trebuchet';\r\n                    font-size: 3.6vw;\r\n                    font-weight: bold;\r\n                    letter-spacing: .05em;\r\n                    color: black;\r\n\r\n                    text-align: left;\r\n                }\r\n\r\n                .item-main h2 {\r\n                    padding: 0 0 2.2em 2.1em;\r\n\r\n                    font-family: 'Trebuchet';\r\n                    font-size: 1.7vw;\r\n                    font-weight: bold;\r\n                    letter-spacing: .06em;\r\n                    color: black;\r\n\r\n                    text-align: left;\r\n                }\r\n\r\n                .item-main .description {\r\n                    position: absolute;\r\n                    top: 76%;\r\n\r\n                    h3 {\r\n                        padding: 0 0 0 3.6em;\r\n    \r\n                        font-family: 'Trebuchet', sans-serif;\r\n                        font-size: 1vw;\r\n                        font-weight: normal;\r\n                        letter-spacing: .06em;\r\n                        color: #b9b9b9;\r\n    \r\n                        text-align: left;\r\n                    }\r\n                }\r\n\r\n                .item-footer {\r\n                    position: relative;\r\n                    top: 1%;\r\n                    box-sizing: border-box;\r\n                    width: 100%;\r\n\r\n          